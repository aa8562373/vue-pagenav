{
  "name": "vue-pagenav",
  "description": "a vuejs pagenav plugin",
  "version": "1.5.0",
  "keywords": [
    "vuejs",
    "pagenav"
  ],
  "scripts": {
    "start": "./node_modules/.bin/npm-run-all -p dev:server dev:client ",
    "build": "rm dist/*.* && NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "dev:server": "node build/dev-server",
    "dev:server-debug": "node --debug build/dev-server",
    "dev:client": "./node_modules/.bin/webpack-dev-server --progress --colors --inline --content-base dist/",
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js"
  },
  "main": "dist/vue-pagenav.js",
  "homepage": "http://html5beta.com/apps/vue-pagenav.html",
  "bugs": "https://github.com/zxdong262/vue-pagenav/issues",
  "author": {
    "email": "zxdong@gmail.com",
    "name": "ZHAO Xudong",
    "url": "http://html5beta.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zxdong262/vue-pagenav"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/zxdong262/vue-pagenav/blob/master/LICENSE"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-factory": "^6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "bootstrap": "^4.0.0-alpha.4",
    "jquery": "^3.1.0",
    "karma": "*",
    "karma-chai-plugins": "*",
    "karma-coverage": "*",
    "karma-mocha": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "koa": "^1.2.4",
    "koa-mount": "^1.3.0",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mocha": "*",
    "npm-run-all": "^3.1.0",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs-prebuilt": "*",
    "unminified-webpack-plugin": "^1.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "vue": "^1.0.26"
  },
  "engines": {
    "node": ">=6"
  }
}
